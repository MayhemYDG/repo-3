  },
  "scripts": {
    "build": "run-s build:roll",
    "build:roll": "node_modules/.bin/rollup -c ./rollup.config.js",
    "dev:roll": "node_modules/.bin/rollup -w -c ./rollup.config.js",
    "test": "run-s test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "zip": "zip -r dist.zip dist",
    "publish:npm": "run-s build && npm publish --access public"
  },
