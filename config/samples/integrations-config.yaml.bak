apiVersion: v1
kind: ConfigMap
metadata:
  name: algo
data:
  jokati.yaml: |
    
    # Ref: https://github.com/a8m/envsubst
    integrations:
      
      # Configuration parameters to be able to connect with RabbitMQ
      alertmanager:
        url: "https://mimir-gateway.tools.fpkmon.com"
        headers:
          X-Scope-OrgID: freepik-company
      
      webhook:
        url: "https://${TEST_USERNAME}:${TEST_PASSWORD}@test.com"
        headers: {}
